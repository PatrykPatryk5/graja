    "better-erela.js-apple": "*",
    "better-erela.js-spotify": "*",
    "colors": "*",
    "discord-api-types": "*",
    "discord-together": "*",
    "discord.js": "*",
    "dotenv": "*",
    "ejs": "*",
    "erela.js": "*",
    "erela.js-deezer": "*",
    "erela.js-facebook": "*",
